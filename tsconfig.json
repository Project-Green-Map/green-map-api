{
    "extends": "@tsconfig/node16/tsconfig.json",
    "include": ["functions/**/*"],
    "exclude": ["node_modules"],
    "compilerOptions": {
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es6",
        "outDir": "dist",
        "rootDir": "./functions"
    },
    "watchOptions": {
        "watchFile": "useFsEvents",
        "watchDirectory": "dynamicprioritypolling",
        "fallbackPolling": "dynamicpriority"
    }
}